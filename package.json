{
  "name": "@mapcreator/maps4news",
  "version": "0.5.5",
  "description": "Maps4News JavaScript Api",
  "main": "dist/bundle.js",
  "repository": "git@github.com:MapCreatorEU/m4n-api.git",
  "author": "Bas Bieling <b.bieling@mapcreator.eu>",
  "license": "BSD-3-Clause",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "mitt": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "contributors": "^0.5.1",
    "dotenv-webpack": "^1.5.2",
    "esdoc": "^0.5.2",
    "eslint": "^4.2.0",
    "fs-extra": "^3.0.1",
    "licensecheck": "^1.3.0",
    "webpack": "^3.1.0",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "generate": "cd src/crud && rm index.js && rows=$(find . -type f -depth 1 -exec basename '{}' \\; | sed s/\\.js//) && for i in $rows; do echo \"export $i from './$i.js';\"; done > index.js",
    "build": "$(npm bin)/webpack --progress",
    "watch": "$(npm bin)/webpack --progress --watch",
    "docs": "$(npm bin)/esdoc",
    "lint": "$(yarn bin)/eslint --no-color --max-warnings 5 --format checkstyle --output-file build/checkstyle.xml src",
    "authors": "$(npm bin)/contributors | grep -v -e '^- [A-Za-z]' | tail -n +4 > AUTHORS.md"
  },
  "contributors": [
    "Bas Bieling <b.bieling@mapcreator.eu> (https://GitHub.com/Mechazawa)"
  ],
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "name": "maps4news",
    "access": [
      "public",
      "protected"
    ],
    "experimentalProposal": {
      "exportExtensions": true
    },
    "scripts": [
      "./dist/bundle.js",
      "./manual/js/sandbox.js"
    ],
    "styles": [
      "./manual/css/sidebar_fix.css",
      "./manual/css/hide_import.css"
    ],
    "manual": {
      "example": [
        "./manual/examples.md",
        "./manual/examples.authentication.md"
      ],
      "installation": [
        "./manual/building.md",
        "./manual/installation.md"
      ]
    }
  }
}
